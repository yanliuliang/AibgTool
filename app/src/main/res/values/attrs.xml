<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--定义一个属性-->
    <attr name="duration" />
    <declare-styleable name="AlphaImageView">
        <attr name="duration" />
    </declare-styleable>
    <!-- 声明一个名为statusNames的字符串数组 -->
    <string-array name="statusNames">
        <item>设备编号</item>
        <item>软件版本</item>
        <item>网络运营商代号</item>
        <item>网络运营商名称</item>
        <item>手机制式</item>
        <item>蜂窝状态信息</item>
        <item>SIM卡的国别</item>
        <item>SIM卡序列号</item>
        <item>SIM卡状态</item>
    </string-array>
    <!-- 声明一个名为simState的字符串数组 -->
    <string-array name="simState">
        <item>状态未知</item>
        <item>无SIM卡</item>
        <item>被PIN加锁</item>
        <item>被PUK加锁</item>
        <item>被NetWork PIN加锁</item>
        <item>已准备好</item>
        <item>未准备好</item>
        <item>SIM损毁</item>
        <item>读写SIM卡出错</item>
        <item>SIM卡被限制使用</item>
    </string-array>
    <!-- 声明一个名为phoneType的字符串数组 -->
    <string-array name="phoneType">
        <item>未知</item>
        <item>GSM</item>
        <item>CDMA</item>
        <item>SIP</item>
    </string-array>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RadiusShapeTextView">
        <attr name="aibg_solidColor" format="reference|color"></attr>
        <attr name="aibg_radius" format="dimension"></attr>
        <attr name="aibg_strokeColor" format="reference|color"></attr>
        <attr name="aibg_strokeWidth" format="dimension"></attr>
        <attr name="aibg_textViewName" format="string"></attr>
    </declare-styleable>
</resources>